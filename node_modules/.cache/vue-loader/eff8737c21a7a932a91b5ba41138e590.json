rial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  margin-top: 5%;\n  font-family: 'Courier New', Courier, monospace;\n}\n#wrapper {\n  height: 100vh;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: space-evenly;\n}\n#name {\n  color: rgb(239, 228, 221);\n  font-size: 3em;\n  text-align: center;\n  margin-bottom: 0;\n  animation: arrival 3s forwards;\n}\n#title {\n  color: rgb(239, 228, 221);\n  font-size: 1.5rem;\n  text-align: center;\n  margin-top: 0;\n  animation: arrival 4s forwards;\n\n}\n.navigation {\n  width: 40vw;\n  height: 40vh;\n  margin: auto;\n  display: flex;\n  flex-direction: column;\n  text-align: center;\n}\n.components {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  animation: arrivalY 0.7s forwards;\n  padding: 1%;\n  margin-top: 10%;\n}\na {\n  color: black;\n  text-decoration: none;\n}\np {\n  font-size: 2vw;\n}\np:hover {\n  animation: underline 1s forwards;\n}\n#triforce {\n  width: 10vw;\n  margin: 0 auto;\n  animation: arrival 1s forwards;\n\n}\n#triforceFull {\n  width: 20vw;\n  margin: 0 auto;\n  animation: arrival 1s ease-in-out;\n}\n\n@keyframes arrival {\n  from {\n    opacity: 0.5;\n  }\n  to {\n    opacity: 1;\n  }\n}\n@keyframes arrivalY {\n  from {\n    transform: translateY(-250px);\n    opacity: 0;\n  }\n  to {\n    transform: translateY(0);\n    opacity: 1;\n  }\n}\n@keyframes leaveY {\n  from {\n    transform: translateY(0);\n    opacity: 1;\n  }\n    to {\n      transform: translateY(-150px);\n      opacity: 0;\n    }\n}\n@keyframes underline {\n  from {\n    text-decoration: none;\n  }\n  to {\n    text-decoration: underline goldenrod 2px;\n    background-color: rgb(239, 228, 221);\n    padding: 10%;\n  }\n}\n</style>"]}]}