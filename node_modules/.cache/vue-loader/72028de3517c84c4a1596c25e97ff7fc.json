n    <contact></contact>\n  </div>\n\n  <div id=\"else\" v-else>\n    <p>Click the buttons above to know more about me</p>\n  </div>\n\n  <Footer></Footer>\n</template>\n\n<script>\nimport Resume from './components/Resume.vue'\nimport Work from './components/Work.vue'\nimport Contact from './components/Contact.vue'\nimport Footer from './components/Footer.vue'\nexport default {\n  components: {\n    Resume,\n    Work,\n    Contact,\n    Footer\n  },\n  data() {\n    return {\n      name: \"Moussa Sy\",\n      title: \"Front-end Developper\",\n      showResume: false,\n      showWork: false,\n      showContact: false,\n      map: undefined\n    }\n  },\n  methods: {\n    toggleResume() {\n      this.showResume = !this.showResume\n\n      if (this.showResume === true) {\n        this.showWork = false\n        this.showContact = false\n      }\n    },\n    toggleWork() {\n      this.showWork = !this.showWork\n\n      if (this.showWork === true) {\n        this.showResume = false\n        this.showContact = false\n      }\n    },\n    toggleContact() {\n      this.showContact = !this.showContact\n\n      if (this.showContact === true) {\n        this.showWork = false\n        this.showResume = false\n      }\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  color: #2c3e50;\n  margin-top: 5%;\n  font-family: 'Courier New', Courier, monospace;\n}\n#name {\n  color: rgb(239, 228, 221);\n  font-size: 3em;\n  text-align: center;\n  margin-bottom: 0;\n  animation: arrival 2s forwards;\n}\n#title {\n  color: rgb(239, 228, 221);\n  font-size: 3rem;\n  text-align: center;\n  margin-top: 0;\n  animation: arrival 3s forwards;\n\n}\n#nav {\n  margin: 10%;\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  justify-content: space-around;\n}\n\nul {\n  list-style: none;\n}\nli {\n  padding: 1% 3%;\n  background-color: rgb(226, 214, 207)\n}\nli:hover {\n  animation: listButton 0.5s forwards;\n}\nli:hover::after {\n  animation: listButton 0.5s reverse;\n}\n\n#resume, #work, #contact {\n  animation: arrival 1s forwards;\n}\n\n@keyframes listButton {\n  from {\n    padding: 1% 3%;\n    background-color: rgb(226, 214, 207)\n  }\n  to {\n    padding: 1% 5%;\n    border-radius: 10%;\n    background-color: black;\n    font-size: large;\n    color: rgb(228, 166, 127)\n  }\n}\n\n@keyframes arrival {\n  from {\n    transform: translateX(-50px);\n    opacity: 0;\n  }\n  to {\n    transform: translateX(0);\n    opacity: 1;\n  }\n}\n</style>\n\n/* On click add a class to the buttons to recreate the hover final state*/\n\n/* same as above for the work component, to know wich one is active */"]}]}